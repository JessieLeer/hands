.main
	padding-top: 46px
	
.good-cell.van-cell
	padding: 2px 0px
	
.ml-30
	margin-left: 30px
	
.total
	color: #EC9738
	
.lot-header
	display: flex
	height: 40px
	line-height: 40px
	padding-left: 16px
	padding-right: 16px
	
.lot-id
	flex: 1
	
.lot-counter
	width: 300px	
	color: #EC9738
	
.lot-title
	font-size: 16px
	background-color: rgb(248,248,248)
	padding-left: 16px
	padding-right: 16px
	height: 40px
	line-height: 40px

.lot-count-wrapper
	padding-left: 16px
	padding-right: 16px
	padding-bottom: 14px
	
.lot-count
	height: 40px
	line-height: 40px
	
.lot-count-wrapper .lot-count:nth-child(2n+1)
	padding-right: 8px
	
.lot-count-wrapper .lot-count:nth-child(2n)
	padding-left: 8px	
	
.lot-count dl
	background-color: rgb(248,248,248)
	
.lot-count dt
	color: rgb(180,180,180)
	
.lot-count dd
	color: rgb(57,57,57)
	
.lot-empty
	padding-top: 100px
	
.add
	padding-left: 60px
	padding-right: 60px
	margin-top: 30px
	
.delete
	width: 200px
	color: #333!important
	
.van-submit-bar__button
	width: 100%
	
.van-dialog
	border-radius: 0